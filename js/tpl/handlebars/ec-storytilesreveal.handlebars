<div class="main-container">
  <a class="close-btn" href="{{landingPageUrl}}">X</a>
  <!-- TODO The article partials have to be adde only on the SS version, not on the CS version -->
  <div class="article-container">{{>partials/theWorldIf/article}}</div>CCC
  <div class="article-reveal-container">
    <ul>
      {{#each tiles}}
      <li class="artical-reveal-tile {{type}} {{animate}}" data-id="{{id}}" id="tile{{id}}">
        <a href="{{url}}">
          <div class="tile-content">
            <h2 class="section">{{section}}</h2>
            <h2 class="title">{{title}}</h2>
            <h2 class="rubrik">{{rubric}}</h2>
            {{#ifvalue type equals="tile-1"}}
            <div class="image-tint"></div>
            <div class="image-grad"></div>
            {{/ifvalue}}
            <img class="image" src="{{img}}">
          </div>
        </a>
      </li>
      {{/each}}
    </ul>
  </div>
</div>











